{
  "name": "localize",
  "description": "A GNU gettext-inspired (but not conformant) localization library for Node.js",
  "homepage": "https://github.com/Larnixva/node-localize",
  "repository": "git@github.com:Larnixva/node-localize",
  "author": "David Ellis <d.f.ellis@ieee.org>",
  "contributors": [
    "Steven Levithan",
    "Felix Geisend√∂rfer",
    "Clint Andrew Hall",
    "Jerry Jalava",
    "Marvin Scholz",
    "Andreas Mieke"
  ],
  "version": "0.4.8",
  "main": "./lib/localize",
  "dependencies": {
    "commander": "~2.3.0"
  },
  "devDependencies": {
    "jshint": "~2.5.4",
    "istanbul": "~0.3.0",
    "nodeunit": "~0.9.0",
    "coveralls": "~2.11.1",
    "mocha": "~1.21.4"
  },
  "engines": {
    "node": "*"
  },
  "bin": {
    "xlocalize": "./bin/xlocalize.js"
  },
  "scripts": {
    "jshint": "jshint --verbose --exclude ./test .",
    "test-lib": "istanbul --print=none cover nodeunit test/localize.test.js && istanbul report text",
    "test-bin": "cd ./test/ && mocha ./xlocalize.test.js",
    "test": "npm run jshint && npm run test-lib && npm run test-bin",
    "coveralls": "cat coverage/lcov.info | coveralls"
  }
}
